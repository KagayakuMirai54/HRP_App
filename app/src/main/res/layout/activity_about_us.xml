<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutUsActivity">

    <ImageView
        android:id="@+id/ivBack_from_aboutUs"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/left_arrow"
        app:civ_border_color="#00D1FF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="center"
        android:text="Detail Info"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <View
        android:id="@+id/view2"
        android:layout_width="300dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/bg_view_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />
    <TextView
        android:id="@+id/tv_iuranKebersihan"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="bottom|left"
        android:text="Iuran Kebersihan"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />
    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="top|fill|left|right"
        android:text="Digunakan untuk menjaga kebersihan di lingkungan kontrakan dengan membayar jasa petugas kebersihan yang mengangkut sampah"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/tv_iuranKebersihan"
        app:layout_constraintTop_toBottomOf="@+id/tv_iuranKebersihan"
        app:layout_constraintVertical_bias="0.0" />
    <View
        android:id="@+id/view"
        android:layout_width="300dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/bg_view_right"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />
    <TextView
        android:id="@+id/tv_iuranWifi"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="bottom|left"
        android:text="Iuran WiFi"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/view" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="top|fill|left|right"
        android:text="Digunakan untuk membayar wifi kontrakan yang tagihannya diberikan setiap bulan pada tanggal 20"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_iuranWifi"
        app:layout_constraintVertical_bias="0.0" />
    <View
        android:id="@+id/view3"
        android:layout_width="300dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/bg_view_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />
    <TextView
        android:id="@+id/tv_iuranKas"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="bottom|left"
        android:text="Iuran Kas"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view3" />
    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="top|fill|left|right"
        android:text="Akan digunakan untuk keperluan membeli alat dan bahan untuk melakukan kegiatan bersih-bersih"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintStart_toStartOf="@+id/tv_iuranKas"
        app:layout_constraintTop_toBottomOf="@+id/tv_iuranKas"
        app:layout_constraintVertical_bias="0.0" />
    <View
        android:id="@+id/view4"
        android:layout_width="300dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/bg_view_right"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />
    <TextView
        android:id="@+id/tv_iuranAir_listrik"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="bottom|left"
        android:text="Iuran Air &amp; Listrik"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/view4" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/ubuntu_light"
        android:gravity="top|fill|left|right"
        android:text="Digunakan untuk membayar tagihan listrik dan PDAM"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_iuranAir_listrik"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>